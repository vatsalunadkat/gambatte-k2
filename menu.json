{
	"items": [
		{
			"name": "GambatteK2 (Gtk2) v0.3",
			"priority": 1,
			"items": [
				{
					"name": "GambatteK2",
					"if": "\"/lib/ld-linux-armhf.so.3\" -f!",
					"action": "./gambatte-k2-armel"
				},
				{
					"name": "GambatteK2",
					"if": "\"/lib/ld-linux-armhf.so.3\" -f",
					"action": "./gambatte-k2-armhf"
				},
				{
					"name": "copy game shortcut to the Library",
					"if": "\"/lib/ld-linux-armhf.so.3\" -f",
					"action": "/bin/cp -a shortcut_* /mnt/us/documents/"
				},
				{
					"name": "enable Bluetooth",
					"action": "./bluetooth.sh enable"
				},
				{
					"name": "disable Bluetooth",
					"action": "./bluetooth.sh disable"
				},
				{
					"name": "GambatteK2 (Debug)",
					"action": "bin/gambatte-k2 > /mnt/us/extensions/gambatte-k2/kual-stdout.log 2> /mnt/us/extensions/gambatte-k2/kual-stderr.log",
					"params": ""
				},
				{
					"name": "View Debug Log",
					"action": "cat /mnt/us/extensions/gambatte-k2/gambatte-debug.log",
					"params": ""
				},
				{
					"name": "View Error Log",
					"action": "cat /mnt/us/extensions/gambatte-k2/kual-stderr.log",
					"params": ""
				},
				{
					"name": "Gambatte-K2 with log script",
					"action": "bin/gambatte-k2.sh",
					"params": ""
				},								
			]
		}
	]
}
